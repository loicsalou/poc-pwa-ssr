{
  "name": "ecdc-web-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run dev",
    "dev": "ng serve --port 4201",
    "prod": "ng serve --prod --port 4200",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "docker build -f dockerfile-withbuild-withssr -t ecdc-web-ui:latest .",
    "docker:run": "docker run -d -p4000:4000 --name ecdc-front ecdc-web-ui:latest",
    "docker:lighthouse": "docker run -itv /tmp/reports:/home/chrome/reports --cap-add=SYS_ADMIN loicsalou/lighthouse:latest",
    "dev:ssr": "ng run ecdc-web-ui:serve-ssr",
    "serve:ssr": "node dist/ecdc-web-ui/server/main.js",
    "build:ssr": "ng build --prod && ng run ecdc-web-ui:server:production",
    "prerender": "ng run ecdc-web-ui:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/localize": "10.0.8",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/platform-server": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@angular/service-worker": "~10.0.5",
    "@nguniversal/express-engine": "~10.0.1",
    "@ngx-translate/core": "13.0.0",
    "@oblique/oblique": "5.0.5",
    "@ts-morph/common": "0.5.2",
    "ajv": "6.12.3",
    "core-js": "^2.0.0",
    "express": "^4.15.2",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.0",
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@nguniversal/builders": "^10.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "26.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.2.1",
    "jest-serializer-vue": "2.0.2",
    "jest-sonar-reporter": "2.0.0",
    "jest-transform-stub": "2.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "jestSonar": {
    "reportPath": "./coverage/sonarQube",
    "reportFile": "sqr.xml",
    "indent": 4,
    "sonar56x": true
  }
}
